//program to swap two numbers using temporary variable
#include <stdio.h>
int main ()
{
    int x, y, temp;
    printf ("\n\t enter values of x, y ");
    scanf("%d%d", &x, &y);
    printf("\n\t before swapping..x=%d, y=%d", x, y);
    temp=x;
    x=y;
    y=temp;
    printf("\n\t after Swapping..x=%d, y=%d", x, y);
    return 0;
}
